{
  "name": "ember-simple-wormhole",
  "version": "0.1.0",
  "description": "Ember addon that wraps in-element",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/nlfurniss/ember-simple-wormhole",
  "license": "MIT",
  "author": "Nathaniel Furniss",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.2",
    "ember-cli-babel": "^7.26.3",
    "ember-cli-htmlbars": "^6.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.8",
    "@babel/plugin-proposal-decorators": "^7.15.8",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.4.2",
    "@embroider/test-setup": "^2.0.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.0.0",
    "ember-cli": "~4.8.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-fastboot": "^3.2.0",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^6.0.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~4.8.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.0.1",
    "ember-try": "^2.0.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-ember": "^11.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "qunit": "^2.14.1",
    "qunit-dom": "^2.0.0",
    "webpack": "^5"
  },
  "engines": {
    "node": ">= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://nlfurniss.github.io/ember-simple-wormhole/"
  }
}
